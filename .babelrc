{
	presets: [
		['env', {
			include: ['es6.object.assign'],
			targets: {
				browsers: ['ie >= 11']
			},
			useBuiltIns: 'usage'
		}],
		['platform', {
			'*': {
				plugins: [
					['transform-runtime', {
						polyfill: false
					}]
				]
			},
			bundle: {
				plugins: [
					['transform-runtime', {
						helpers: false
					}]
				]
			}
		}]
	],
	plugins: [
		['transform-object-rest-spread', { useBuiltIns: true }]
	]
}
